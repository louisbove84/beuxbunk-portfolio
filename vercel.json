{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "headers": [
    {
      "source": "/api/frame-image",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/carboncheck",
      "destination": "/carboncheck/index.html"
    },
    {
      "source": "/carboncheck/:path*",
      "destination": "/carboncheck/index.html"
    }
  ]
}